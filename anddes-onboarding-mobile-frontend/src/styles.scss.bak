/* You can add global styles to this file, and also import other style files */
@use '@angular/material' as mat;
// Plus imports for other components in your app.

// Include the common styles for Angular Material. We include this here so that you only
// have to load a single css file for Angular Material in your app.
// Be sure that you only ever include this mixin once!
@include mat.core();
/* You can add global styles to this file, and also import other style files */
html,
body {
  height: 100%;
}
@font-face {
  font-family: 'Neo Sans Std';
  src: url(assets/font/Neo_Sans_Std_Regular.otf) format("opentype");
}

body {
  font-family: Monserrat, "Neo Sans Std", sans-serif;
  margin: 0;
  height: 100%;
  @include mat.theme((color: (theme-type: light,
        primary: mat.$azure-palette,
        tertiary: mat.$blue-palette,
      ),
      typography: Monserrat,
      density: 0,
    ));
}

:root {
  /* Colors: */
  ---00539b: #00539B;
  ---383a38: #383A38;
  --unnamed-color-eeb500: #EEB500;
  --gray: #DFE3E4;
  --sucess: #408D1B;
  --container-high-primerdia: #CAF0B9;
  --container-low-primerdia: #EDF9E8;
  --container-high-antes: #C8E9FF;
  --container-low-antes: #EBF7FC;
  --text-info: #C2862B;
  --container-tex: #FEF3DE;
  --error: #FF444E;
  --surface-white: #FFFFFF;
  --backgroung-web: #FAFAFA;
  --menu: #212121;
  --surface-grey: #F5F5F5;
  --unnamed-color-424242: #424242;

  /* Font/text values */
  --unnamed-font-family-neo-sans-std: 'Neo Sans Std';
  --unnamed-font-family-montserrat: Montserrat;
  --unnamed-font-style-normal: normal;
  --unnamed-font-weight-normal: normal;
  --unnamed-font-size-10: 10px;
  --unnamed-font-size-12: 12px;
  --unnamed-font-size-14: 14px;
  --unnamed-font-size-16: 16px;
  --unnamed-font-size-22: 22px;
  --unnamed-font-size-32: 32px;
  --unnamed-font-size-56: 56px;
  --unnamed-character-spacing-0: 0px;
  --unnamed-character-spacing-0-5: 0.5px;
  --unnamed-character-spacing-1-8: 1.8px;
  --unnamed-character-spacing-0-02: 0.02px;
  --unnamed-character-spacing-0-04: 0.04px;
  --unnamed-character-spacing--0-81: -0.81px;
  --unnamed-character-spacing-0-06: 0.06px;
  --unnamed-character-spacing-0-05: 0.05px;
  --unnamed-character-spacing-0-13: 0.13px;
  --unnamed-line-spacing-16: 16px;
  --unnamed-line-spacing-20: 20px;
  --unnamed-line-spacing-24: 24px;
  --unnamed-line-spacing-28: 28px;
  --unnamed-line-spacing-40: 40px;
  --unnamed-line-spacing-64: 64px;
  --unnamed-text-transform-uppercase: uppercase;
}

/* Character Styles */
.displayl-neosans-56pt {
  font-family: var(--unnamed-font-family-neo-sans-std);
  font-style: var(--unnamed-font-style-normal);
  font-weight: var(--unnamed-font-weight-normal);
  font-size: var(--unnamed-font-size-56);
  line-height: var(--unnamed-line-spacing-64);
  letter-spacing: var(--unnamed-character-spacing--0-81);
  color: var(--unnamed-color-424242);
}

.headline-l-neosans-32pt {
  font-family: var(--unnamed-font-family-neo-sans-std);
  font-style: var(--unnamed-font-style-normal);
  font-weight: var(--unnamed-font-weight-normal);
  font-size: var(--unnamed-font-size-32);
  line-height: var(--unnamed-line-spacing-40);
  letter-spacing: var(--unnamed-character-spacing-0);
  color: var(--unnamed-color-424242);
}

.title-l-neosans-22pt {
  font-family: var(--unnamed-font-family-neo-sans-std);
  font-style: var(--unnamed-font-style-normal);
  font-weight: var(--unnamed-font-weight-normal);
  font-size: var(--unnamed-font-size-22);
  line-height: var(--unnamed-line-spacing-28);
  letter-spacing: var(--unnamed-character-spacing-0);
  color: var(--unnamed-color-424242);
}

.title-s-neosans-14pt {
  font-family: var(--unnamed-font-family-neo-sans-std);
  font-style: var(--unnamed-font-style-normal);
  font-weight: var(--unnamed-font-weight-normal);
  font-size: var(--unnamed-font-size-14);
  line-height: var(--unnamed-line-spacing-20);
  letter-spacing: var(--unnamed-character-spacing-0-02);
  color: var(--unnamed-color-424242);
}

.title-m-neosans-16pt {
  font-family: var(--unnamed-font-family-neo-sans-std);
  font-style: var(--unnamed-font-style-normal);
  font-weight: var(--unnamed-font-weight-normal);
  font-size: var(--unnamed-font-size-16);
  line-height: var(--unnamed-line-spacing-24);
  letter-spacing: var(--unnamed-character-spacing-0-04);
  color: var(--unnamed-color-424242);
}

.body-l-monserrant-16pt {
  font-family: var(--unnamed-font-family-montserrat);
  font-style: var(--unnamed-font-style-normal);
  font-weight: var(--unnamed-font-weight-normal);
  font-size: var(--unnamed-font-size-16);
  line-height: var(--unnamed-line-spacing-24);
  letter-spacing: var(--unnamed-character-spacing-0-13);
  color: var(--unnamed-color-424242);
}

.body-m-monserrat-14pt {
  font-family: var(--unnamed-font-family-montserrat);
  font-style: var(--unnamed-font-style-normal);
  font-weight: var(--unnamed-font-weight-normal);
  font-size: var(--unnamed-font-size-14);
  line-height: var(--unnamed-line-spacing-20);
  letter-spacing: var(--unnamed-character-spacing-0-05);
  color: var(--unnamed-color-424242);
}

.body-s-monserrant-12pt {
  font-family: var(--unnamed-font-family-montserrat);
  font-style: var(--unnamed-font-style-normal);
  font-weight: var(--unnamed-font-weight-normal);
  font-size: var(--unnamed-font-size-12);
  line-height: var(--unnamed-line-spacing-16);
  letter-spacing: var(--unnamed-character-spacing-0-06);
  color: var(--unnamed-color-424242);
}

.button-neosans-12pt {
  font-family: var(--unnamed-font-family-neo-sans-std);
  font-style: var(--unnamed-font-style-normal);
  font-weight: var(--unnamed-font-weight-normal);
  font-size: var(--unnamed-font-size-12);
  line-height: var(--unnamed-line-spacing-16);
  letter-spacing: var(--unnamed-character-spacing-1-8);
  color: var(--unnamed-color-424242);
  text-transform: var(--unnamed-text-transform-uppercase);
}

.over-neosans-10pt {
  font-family: var(--unnamed-font-family-neo-sans-std);
  font-style: var(--unnamed-font-style-normal);
  font-weight: var(--unnamed-font-weight-normal);
  font-size: var(--unnamed-font-size-10);
  line-height: var(--unnamed-line-spacing-16);
  letter-spacing: var(--unnamed-character-spacing-0-5);
  color: var(---383a38);
}
.entity-card {
  width: 345px;
  border: 1px solid #707070;
  border-radius: 12px;
}
.entity-cover {
  width: 343px;
  height: 220px;
  object-fit: cover;
}
#buttonLogin {
  display: block;
  width: 115px;
  height: 54px;
  border-radius: 4px;
  background-color: #00539B;

  text-align: left;
  font: normal normal normal 12px/16px Neo Sans Std;
  letter-spacing: 1.8px;
  color: #FFFFFF;
  text-transform: uppercase;
  opacity: 1;
}